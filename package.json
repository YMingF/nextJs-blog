{
   "name": "nextjs-blog-1",
   "version": "0.1.0",
   "private": true,
   "scripts": {
      "dev": "concurrently \" next dev\" \"npm run compile:watch \" ",
      "compile:watch": "babel -w ./src --out-dir dist --extensions .ts,.tsx",
      "compile": "babel ./src --out-dir dist --extensions .ts,.tsx",
      "build": "cross-env BROWSERSLIST_IGNORE_OLD_DATA=1 next build",
      "start": "next start",
      "m:create": "typeorm migration:create -n",
      "m:run": "typeorm migration:run",
      "m:revert": "typeorm migration:revert",
      "e:create": "typeorm entity:create -n",
      "typeorm:build": "babel -w ./src --out-dir dist --extensions .ts,.tsx",
      "dist": "node dist/seed.js"
   },
   "dependencies": {
      "@ant-design/cssinjs": "^1.21.0",
      "@babel/cli": "^7.10.1",
      "antd": "^5.19.2",
      "axios": "^0.19.2",
      "boring-avatars": "^1.10.2",
      "cross-env": "^7.0.3",
      "gray-matter": "^4.0.2",
      "lodash": "^4.7.4",
      "marked": "^1.1.0",
      "nanoid": "^5.0.7",
      "next": "14.2.1",
      "next-iron-session": "^4.2.0",
      "pg": "^8.2.1",
      "react": "^18.2.0",
      "react-dom": "^18.2.0",
      "react-markdown": "^9.0.1",
      "reflect-metadata": "^0.1.13",
      "rehype-prism": "^2.3.2",
      "remark": "^15.0.1",
      "remark-breaks": "^4.0.0",
      "remark-gfm": "^4.0.0",
      "remark-html": "^16.0.1",
      "sass": "^1.26.5",
      "typeorm": "^0.2.25",
      "ua-parser-js": "^0.7.21"
   },
   "devDependencies": {
      "@babel/plugin-proposal-decorators": "^7.10.1",
      "@types/axios": "^0.14.0",
      "@types/lodash": "^4.14.168",
      "@types/marked": "^0.7.4",
      "@types/node": "^14.0.6",
      "@types/react": "18.3.3",
      "@types/react-dom": "^16.9.8",
      "@types/ua-parser-js": "^0.7.33",
      "concurrently": "^5.2.0",
      "next-images": "^1.4.0",
      "tailwindcss": "^3.4.4",
      "typescript": "^4.5.4"
   }
}
